{
	"name": "frontend-react",
	"version": "1.0.0",
	"description": "RapidMiner Streaming Dashboard",
	"scripts": {
		"start": "npm run clean && cross-env NODE_ENV=development nodemon --exec babel-node --config nodemon.json --extensions \".ts,.js\" --presets='@babel/preset-env,@babel/preset-typescript' src/ts/server/dev.server.ts",
		"clean": "rimraf ../src/main/resources/com/rapidminer/extension/resources/html5/dashboard/dist",
		"prepare-build": "npm run clean && mkdirp ../src/main/resources/com/rapidminer/extension/resources/html5/dashboard/dist",
		"build:development": "npm run prepare-build && cross-env NODE_ENV=development webpack",
		"build:production": "npm run prepare-build && cross-env NODE_ENV=production webpack",
		"lint": "eslint \"src/**/*.ts\" \"src/**/*.tsx\" \"webpack/**/*.js\"",
		"lintfix": "eslint --fix \"src/**/*.ts\" \"src/**/*.tsx\" \"webpack/**/*.js\"",
		"lint:style": "stylelint \"./src/stylesheets/**/*.less\" --syntax less",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.less\" \"webpack/**/*.js\"",
		"checkstyle": "eslint -c .eslintrc \"src/**/*.ts\" \"src/**/*.tsx\" -f checkstyle > ci/eslint.xml",
		"test": "jest --no-cache --config jest.config.json",
		"test:watch": "jest --no-cache --config jest.config.json --watch --colors",
		"test:coverage": "jest --no-cache --config jest.config.json --coverage"
	},
	"author": "infore@rapidminer.com",
	"license": "UNLICENSED",
